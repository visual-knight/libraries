{
  "name": "visual-knight",
  "version": "1.0.0",
  "description": "Collection of libraries for Visual Knight (https://www.visual-knight.io)",
  "private": true,
  "scripts": {
    "test": "jest --no-cache --config jest.config.json",
    "test:watch:all": "jest --no-cache --config jest.config.json --watchAll --collectCoverage",
    "bootstrap": "lerna bootstrap",
    "prepublishOnly": "npm run clean && lerna run prepare",
    "clean": "rimraf packages/**/lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visual-knight/libraries.git"
  },
  "keywords": [
    "visual-knight",
    "visual",
    "regression",
    "testing",
    "screenshot"
  ],
  "author": "Mitko Tschimev <mitko@dev-thought.cool>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/visual-knight/libraries/issues"
  },
  "homepage": "https://github.com/visual-knight/libraries#readme",
  "dependencies": {
    "lerna": "^3.2.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.4",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
